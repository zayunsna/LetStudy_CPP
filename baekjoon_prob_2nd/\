#include "/home/icegpu/HK/HKTool.h"
#include <bits/stdc++.h>
using namespace std;

int n;
int a;
string str;
int c[3];
int m[3];
pair<int,int> p;

void split(string s, int &now){

	int m_now = atoi(s.substr(0,2).c_str());
	int s_now = atoi(s.substr(3,2).c_str());
	cout << m_now << " : " << s_now << "\n";
	now = m_now*60 + s_now;
}

void baekjoon_2852(){

	ios_base::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);

	cin >> n;
	for(int i = 0; i < n; ++i){
		int ts_now = 0;
		cin >> a >> str;
		c[a]++;
		split(str, ts_now);
		cout << a << "  " << ts_now << "\n";
		if(c[1] != c[2]) p = make_pair(a, ts_now);
	}

}
